<view><block wx:for="{{goods}}" wx:for-item="g" wx:for-index="idx" wx:key="idx"><block><view class="uni-index-list-cell"><view data-event-opts="{{[['tap',[['goodClick',['$0'],[[['goods','',idx]]]]]]]}}" class="uni-good-list" bindtap="__e"><image class="uni-good-list-logo" lazy-load src="{{g.main_pic}}"></image><view class="uni-good-list-body"><view class="uni-good-list-text-top">{{g.dtitle}}</view><view class="uni-good-list-text-bottom"><block wx:if="{{g.istmall}}"><text class="good-price-favour">{{"天猫价￥"+g.original_price}}</text></block><block wx:else><text class="good-price-favour">{{"淘宝价￥"+g.original_price}}</text></block><text class="good-sell-number">{{"已售"+g.sales_num+"件"}}</text></view><view class="uni-good-list-text-bottom"><text class="good-price">{{"券后￥"+g.price}}</text><text class="good-quan">{{"￥"+g.coupon_price+"元券"}}</text></view></view></view></view></block></block><block wx:if="{{invalidGoods.length>0}}"><view class="clear-invalid-good"><text data-event-opts="{{[['tap',[['clearInvalidGood',['$event']]]]]}}" bindtap="__e">清空失效商品</text></view></block><block wx:for="{{invalidGoods}}" wx:for-item="g" wx:for-index="idx" wx:key="idx"><block><view class="uni-index-list-cell"><view data-event-opts="{{[['tap',[['goodClick',['$0'],[[['invalidGoods','',idx]]]]]]]}}" class="uni-good-list" bindtap="__e"><image class="uni-good-list-logo" lazy-load src="{{g.main_pic}}"></image><view class="uni-good-list-body"><view class="uni-good-list-text-top">{{g.dtitle}}</view><view class="uni-good-list-text-bottom"><block wx:if="{{g.istmall}}"><text class="good-price-favour">{{"天猫价￥"+g.original_price}}</text></block><block wx:else><text class="good-price-favour">{{"淘宝价￥"+g.original_price}}</text></block><text class="good-sell-number">{{"已售"+g.sales_num+"件"}}</text></view><view class="uni-good-list-text-bottom"><text class="good-price">{{"券后￥"+g.price}}</text><text class="good-quan">{{"￥"+g.coupon_price+"元券"}}</text></view></view></view></view></block></block></view>