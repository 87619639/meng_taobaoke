<view class="uni-tab-bar"><view class="mp-search-box"><input class="ser-input" type="text" value="输入关键字搜索" disabled/></view><view class="titleNview-placing"></view><scroll-view class="uni-swiper-tab" id="tab-bar" scroll-x scroll-left="{{scrollLeft}}"><block wx:for="{{tabs}}" wx:for-item="tab" wx:for-index="index" wx:key="index"><view class="{{['swiper-tab-list',(tabIndex==index)?'active':'']}}" id="{{'tab_'+tab.id}}" data-event-opts="{{[['tap',[['tapTab',['$0',index],[[['tabs','',index]]]]]]]}}" bindtap="__e">{{''+tab.name+''}}</view></block></scroll-view><swiper class="swiper-box touch-action-none" current="{{tabIndex}}" skip-hidden-item-layout="true" duration="{{300}}" data-event-opts="{{[['change',[['changeTab',['$event']]]]]}}" bindchange="__e"><block wx:for="{{goods}}" wx:for-item="good" wx:for-index="index" wx:key="index"><swiper-item><scroll-view class="good-list touch-action-none" scroll-top="{{scrollTop}}" scroll-y data-event-opts="{{[['scroll',[['scroll',['$event']]]],['scrolltolower',[['loadMore',['$0'],[[['goods','',index,'cid']]]]]]]}}" bindscroll="__e" bindscrolltolower="__e"><view class="carousel-section"><swiper class="carousel" circular data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{carouselList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item data-event-opts="{{[['tap',[['navToDetailPage',[['o',['title','轮播广告']]]]]]]}}" class="carousel-item" bindtap="__e"><image src="{{item.src}}"></image></swiper-item></block></swiper><view class="swiper-dots"><text class="num">{{swiperCurrent+1}}</text><text class="sign">/</text><text class="num">{{swiperLength}}</text></view></view><block wx:if="{{good.cid==0}}"><view><block wx:if="{{banners.length>0}}"><uni-swiper-dot vue-id="{{'1-'+index}}" bind:__l="__l" vue-slots="{{['default']}}"><swiper class="banner-swiper" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}"><block wx:for="{{banners}}" wx:for-item="banner" wx:for-index="index" wx:key="index"><swiper-item><view class="banner-swiper-item"><image src="{{banner.image}}" lazy-load mode="aspectFill" data-event-opts="{{[['tap',[['bannerClick',['$0'],[[['banners','',index]]]]]]]}}" bindtap="__e"></image></view></swiper-item></block></swiper></uni-swiper-dot></block><view class="grid-list"><uni-grid vue-id="{{'2-'+index}}" options="{{activitys}}" show-border="{{false}}" columnNum="4" data-event-opts="{{[['^click',[['actClick']]]]}}" bind:click="__e" bind:__l="__l"></uni-grid></view><view class="active-title-image"><image src="{{index_1.image}}" lazy-load mode="aspectFill" data-event-opts="{{[['tap',[['bannerClick',['$0'],['index_1']]]]]}}" bindtap="__e"></image></view></view></block><block wx:if="{{good.cid>0}}"><view class="cate-grid-list"><uni-grid vue-id="{{'3-'+index}}" options="{{gcategorys[index].sub_categorys}}" show-border="{{false}}" columnNum="5" data-event-opts="{{[['^click',[['tagClick']]]]}}" bind:click="__e" bind:__l="__l"></uni-grid></view></block><block wx:for="{{good.data}}" wx:for-item="g" wx:for-index="idx" wx:key="idx"><block><view class="uni-index-list-cell"><view data-event-opts="{{[['tap',[['goodClick',['$0'],[[['goods','',index],['data','',idx]]]]]]]}}" class="uni-good-list" bindtap="__e"><image class="uni-good-list-logo" lazy-load src="{{g.main_pic}}"></image><view class="uni-good-list-body"><view class="uni-good-list-text-top">{{g.dtitle}}</view><view class="uni-good-list-text-bottom"><block wx:if="{{g.istmall}}"><text class="good-price-favour">{{"天猫价￥"+g.original_price}}</text></block><block wx:else><text class="good-price-favour">{{"淘宝价￥"+g.original_price}}</text></block><text class="good-sell-number">{{"已售"+g.sales_num+"件"}}</text></view><view class="uni-good-list-text-bottom"><text class="good-price">{{"券后￥"+g.price}}</text><text class="good-quan">{{"￥"+g.coupon_price+"元券"}}</text></view></view></view></view></block></block><uni-load-more vue-id="{{'4-'+index}}" status="{{loadmoreStatue}}" contentText="{{loadingText}}" bind:__l="__l"></uni-load-more></scroll-view></swiper-item></block></swiper><uni-floating-button vue-id="5" visible="{{showFloatButton}}" data-event-opts="{{[['^click',[['gotTop']]]]}}" bind:click="__e" bind:__l="__l"></uni-floating-button></view>