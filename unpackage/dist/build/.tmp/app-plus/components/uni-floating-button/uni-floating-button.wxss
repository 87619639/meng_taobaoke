
.fab-box {
	position: fixed;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	z-index: 2;
}
.fab-box.fab {
	z-index: 10;
}
.fab-box.fab.rightBottom {
	right: 30rpx;
	bottom: 260rpx;
}
.fab-circle {
	opacity: 0.5;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	position: absolute;
	width: 80rpx;
	height: 80rpx;
	background: #3c3e49;
	/* background: #5989b9; */
	border-radius: 50%;
	-webkit-box-shadow: 0 0 5px 2px rgba(0, 0, 0, 0.2);
	        box-shadow: 0 0 5px 2px rgba(0, 0, 0, 0.2);
	z-index: 11;
}
.fab-circle.right {
	right: 0;
}
.fab-arrowup-color{
	color: #FFFFFF;
}

